@using EB.Common
@using GroupflyGroup.Platform.ObjectFramework;
@using GroupflyGroup.License;
@using GroupflyGroup.Platform.ObjectFramework.Persistence
@using GroupflyGroup.Platform.Web.Common
@model GroupflyGroup.Platform.Web.Models.PropertyViewModel

    <div style="width: 20%; min-width: 80px; text-align: right; display: inline-block; padding: 4px;">@Model.Label :</div>
    <div style="width: 75%; text-align: left; display: inline-block; padding: 4px;">
        @if (Model.ReadOnly || Model.State != "edit")
        {
            <span>@(this.CustomControlDisplay(Model))</span>
        }
        else
        {
            @(this.CustomControl(Model))
        }
    </div>

